{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "properties": {
    "affinity": {
      "required": [],
      "title": "affinity",
      "type": "object"
    },
    "env": {
      "description": "environment variables",
      "properties": {
        "aqiStandard": {
          "default": "uk",
          "description": "AQI standard: uk, epa, mep, nepm",
          "enum": [
            "uk",
            "epa",
            "mep",
            "nepm"
          ],
          "required": [],
          "title": "aqiStandard"
        },
        "debug": {
          "default": false,
          "description": "enable debug output",
          "title": "debug",
          "type": "boolean"
        },
        "distanceUnit": {
          "default": "km",
          "description": "Distance in km, mi",
          "enum": [
            "km",
            "mi"
          ],
          "required": [],
          "title": "distanceUnit"
        },
        "indoorLocation": {
          "default": "",
          "description": "Physical location of indoor Ecowitt gateway",
          "title": "indoorLocation",
          "type": "string"
        },
        "irradianceUnit": {
          "default": "wm2",
          "description": "Irradiance in wm2, lx, fc",
          "enum": [
            "wm2",
            "lx",
            "fc"
          ],
          "required": [],
          "title": "irradianceUnit"
        },
        "outdoorLocation": {
          "default": "",
          "description": "Physical location of outdoor Ecowitt sensor array",
          "title": "outdoorLocation",
          "type": "string"
        },
        "pressureUnit": {
          "default": "hpa",
          "description": "Pressure in hpa, in or mmhg",
          "enum": [
            "hpa",
            "in",
            "mmhg"
          ],
          "required": [],
          "title": "pressureUnit"
        },
        "rainUnit": {
          "default": "mm",
          "description": "Rainfall in mm or in",
          "enum": [
            "mm",
            "in"
          ],
          "required": [],
          "title": "rainUnit"
        },
        "temp1Location": {
          "default": "",
          "description": "Physical location of Ecowitt channel 1 temperature sensor",
          "title": "temp1Location",
          "type": "string"
        },
        "temp2Location": {
          "default": "",
          "description": "Physical location of Ecowitt channel 2 temperature sensor",
          "title": "temp2Location",
          "type": "string"
        },
        "temp3Location": {
          "default": "",
          "description": "Physical location of Ecowitt channel 3 temperature sensor",
          "title": "temp3Location",
          "type": "string"
        },
        "temp4Location": {
          "default": "",
          "description": "Physical location of Ecowitt channel 4 temperature sensor",
          "title": "temp4Location",
          "type": "string"
        },
        "temp5Location": {
          "default": "",
          "description": "Physical location of Ecowitt channel 5 temperature sensor",
          "title": "temp5Location",
          "type": "string"
        },
        "temp6Location": {
          "default": "",
          "description": "Physical location of Ecowitt channel 6 temperature sensor",
          "title": "temp6Location",
          "type": "string"
        },
        "temp7Location": {
          "default": "",
          "description": "Physical location of Ecowitt channel 7 temperature sensor",
          "title": "temp7Location",
          "type": "string"
        },
        "temp8Location": {
          "default": "",
          "description": "Physical location of Ecowitt channel 8 temperature sensor",
          "title": "temp8Location",
          "type": "string"
        },
        "temperatureUnit": {
          "default": "c",
          "description": "Temperature in c, f or k",
          "enum": [
            "c",
            "f",
            "k"
          ],
          "required": [],
          "title": "temperatureUnit"
        },
        "windUnit": {
          "default": "kmh",
          "description": "Wind speed in kmh, mph, ms, knots, fps",
          "enum": [
            "kmh",
            "mph",
            "ms",
            "knots",
            "fps"
          ],
          "required": [],
          "title": "windUnit"
        }
      },
      "required": [],
      "title": "env",
      "type": "object"
    },
    "fullnameOverride": {
      "default": "",
      "title": "fullnameOverride",
      "type": "string"
    },
    "global": {
      "description": "Global values are values that can be accessed from any chart or subchart by exactly the same name.",
      "required": [],
      "title": "global",
      "type": "object"
    },
    "image": {
      "properties": {
        "pullPolicy": {
          "default": "IfNotPresent",
          "description": "image pull policy",
          "title": "pullPolicy",
          "type": "string"
        },
        "repository": {
          "default": "djjudas21/ecowitt-exporter",
          "description": "image repository",
          "title": "repository",
          "type": "string"
        },
        "tag": {
          "default": "",
          "description": "image tag",
          "title": "tag",
          "type": "string"
        }
      },
      "required": [],
      "title": "image",
      "type": "object"
    },
    "imagePullSecrets": {
      "items": {
        "required": []
      },
      "title": "imagePullSecrets",
      "type": "array"
    },
    "ingress": {
      "description": "Enable and configure ingress settings for the chart under this key.",
      "properties": {
        "annotations": {
          "required": [],
          "title": "annotations",
          "type": "object"
        },
        "className": {
          "default": "",
          "title": "className",
          "type": "string"
        },
        "enabled": {
          "default": false,
          "description": "Enable ingress",
          "title": "enabled",
          "type": "boolean"
        },
        "hosts": {
          "description": "kubernetes.io/ingress.class: nginx\nkubernetes.io/tls-acme: \"true\"",
          "items": {
            "anyOf": [
              {
                "properties": {
                  "host": {
                    "default": "chart-example.local",
                    "title": "host",
                    "type": "string"
                  },
                  "paths": {
                    "items": {
                      "anyOf": [
                        {
                          "properties": {
                            "path": {
                              "default": "/",
                              "title": "path",
                              "type": "string"
                            },
                            "pathType": {
                              "default": "Prefix",
                              "title": "pathType",
                              "type": "string"
                            }
                          },
                          "required": [],
                          "type": "object"
                        }
                      ],
                      "required": []
                    },
                    "title": "paths",
                    "type": "array"
                  }
                },
                "required": [],
                "type": "object"
              }
            ],
            "required": []
          },
          "title": "hosts",
          "type": "array"
        },
        "tls": {
          "items": {
            "required": []
          },
          "title": "tls",
          "type": "array"
        }
      },
      "required": [],
      "title": "ingress",
      "type": "object"
    },
    "nameOverride": {
      "default": "",
      "title": "nameOverride",
      "type": "string"
    },
    "nodeSelector": {
      "required": [],
      "title": "nodeSelector",
      "type": "object"
    },
    "podAnnotations": {
      "required": [],
      "title": "podAnnotations",
      "type": "object"
    },
    "podSecurityContext": {
      "required": [],
      "title": "podSecurityContext",
      "type": "object"
    },
    "prometheusRules": {
      "properties": {
        "aqiWarn": {
          "default": 7,
          "description": "Warn when AQI exceeds this value, on whatever scale is set in aqiStandard",
          "title": "aqiWarn",
          "type": "integer"
        },
        "batteryAlerts": {
          "default": true,
          "description": "Enable battery alerts",
          "title": "batteryAlerts",
          "type": "boolean"
        },
        "enabled": {
          "default": false,
          "description": "Enable Prometheus rules for Prometheus Operator",
          "title": "enabled",
          "type": "boolean"
        },
        "pm25Warn": {
          "default": 30,
          "description": "Warn when PM2.5 concentration exceeds this value, in µg/m³",
          "title": "pm25Warn",
          "type": "integer"
        },
        "tempHigh": {
          "default": 40,
          "description": "Warn when temperature exceeds this value, in whatever unit is set in tempUnit",
          "title": "tempHigh",
          "type": "integer"
        },
        "tempLow": {
          "default": 0,
          "description": "Warn when temperature falls below this value, in whatever unit is set in tempUnit",
          "title": "tempLow",
          "type": "integer"
        },
        "uvExtreme": {
          "default": 11,
          "description": "Warn when UV index exceeds this Extreme value",
          "title": "uvExtreme",
          "type": "integer"
        },
        "uvHigh": {
          "default": 8,
          "description": "Warn when UV index exceeds this High value",
          "title": "uvHigh",
          "type": "integer"
        },
        "voltageWarning": {
          "default": 1.5,
          "description": "Warn when battery voltage drops below this threshold, in volts",
          "title": "voltageWarning",
          "type": "number"
        },
        "weatherAlerts": {
          "default": true,
          "description": "Enable weather alerts",
          "title": "weatherAlerts",
          "type": "boolean"
        },
        "windWarn": {
          "default": 50,
          "description": "Warn when wind speed exceeds this value, in whatever unit is set in windUnit",
          "title": "windWarn",
          "type": "integer"
        }
      },
      "required": [],
      "title": "prometheusRules",
      "type": "object"
    },
    "replicaCount": {
      "default": 1,
      "description": "Number of replicas",
      "title": "replicaCount",
      "type": "integer"
    },
    "resources": {
      "required": [],
      "title": "resources",
      "type": "object"
    },
    "securityContext": {
      "required": [],
      "title": "securityContext",
      "type": "object"
    },
    "service": {
      "properties": {
        "port": {
          "default": 8088,
          "title": "port",
          "type": "integer"
        },
        "type": {
          "default": "ClusterIP",
          "title": "type",
          "type": "string"
        }
      },
      "required": [],
      "title": "service",
      "type": "object"
    },
    "serviceAccount": {
      "properties": {
        "annotations": {
          "description": "Annotations to add to the service account",
          "required": [],
          "title": "annotations",
          "type": "object"
        },
        "create": {
          "default": true,
          "description": "Specifies whether a service account should be created",
          "title": "create",
          "type": "boolean"
        },
        "name": {
          "default": "",
          "description": "The name of the service account to use.",
          "title": "name",
          "type": "string"
        }
      },
      "required": [],
      "title": "serviceAccount",
      "type": "object"
    },
    "serviceMonitor": {
      "properties": {
        "enabled": {
          "default": false,
          "description": "Enable Service Monitor for Prometheus Operator",
          "title": "enabled",
          "type": "boolean"
        },
        "interval": {
          "default": "60s",
          "description": "Metrics scrape interval. Should be the same as the Ecowitt gateway submission interval",
          "title": "interval",
          "type": "string"
        }
      },
      "required": [],
      "title": "serviceMonitor",
      "type": "object"
    },
    "tolerations": {
      "items": {
        "required": []
      },
      "title": "tolerations",
      "type": "array"
    }
  },
  "required": [],
  "type": "object"
}